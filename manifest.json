{
  "manifest_version": 3,
  "name": "PhiBot",
  "version": "1.0",
  "description": "Check for new internships using filters.",
  "permissions": ["storage", "scripting", "activeTab", "webNavigation"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.jpeg"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"]
    }
  ]
}
